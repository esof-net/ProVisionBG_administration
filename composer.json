{
  "name": "provision/administration",
  "description": "ProVision Administration - laravel package",
  "keywords": [
    "administration",
    "modular",
    "laravel"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Venelin Iliev",
      "homepage": "http://veneliniliev.com"
    }
  ],
  "support": {
    "issues": "https://github.com/ProVisionBG/administration/issues",
    "source": "https://github.com/ProVisionBG/administration"
  },
  "homepage": "http://provision.bg",
  "type": "library",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "laravel/helpers": "^1.7",
    "provision/media-manager": "^11.0",
    "provision/visual-builder": "dev-master",
    "provision/translation": "^11.0",
    "mcamara/laravel-localization": "^2.2.1",
    "zizaco/entrust": "dev-master",
    "caffeinated/modules": "dev-master",
    "lavary/laravel-menu": "^1.7",
    "cviebrock/eloquent-sluggable": "^11.0.1",
    "diglactic/laravel-breadcrumbs": "^9.0.0",
    "yajra/laravel-datatables-oracle": "~11.1.5",
    "yajra/laravel-datatables-html": "^11.8.0",
    "venturecraft/revisionable": "1.*",
    "spatie/laravel-html": "^3.11.1",
    "kris/laravel-form-builder": "^1.7",
    "astrotomic/laravel-translatable": "v11.15",
    "rutorika/sortable": "^4.1",
    "barryvdh/laravel-debugbar": "^3.1",
    "arcanedev/log-viewer": "^11.0.1",
    "laravel-lang/lang": "~3.0",
    "intervention/imagecache": "dev-master"
  },
  "require-dev": {
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ProVision\\Administration\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ProVision\\Administration\\": "app/",
      "ProVision\\Administration\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ProVision\\Administration\\Providers\\AdministrationServiceProvider"
      ],
      "aliases": {
        "Administration": "ProVision\\Administration\\Facades\\Administration",
        "AdministrationMenu": "ProVision\\Administration\\Facades\\AdministrationMenu",
        "Settings": "ProVision\\Administration\\Facades\\Settings",
        "Dashboard": "ProVision\\Administration\\Dashboard"
      }
    }
  },
  "repositories": [
    {
      "url": "../media-manager",
      "type": "git"
    },
    {
      "url": "../translation",
      "type": "path"
    },
    {
      "url": "https://github.com/bee-interactive/modules",
      "type": "git"
    },
    {
      "url": "https://github.com/mrjohnmain/imagecache",
      "type": "git"
    }
  ],  
  "scripts": {
    "post-install-cmd": [
    ],
    "post-update-cmd": [
    ],
    "test": "phpunit"
  },
  "config": {
    "cache-dir": "/app/.composer/cache/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
